{
  "name": "oxigraph_tests",
  "description": "Build a tests for Oxigraph JS",
  "private": true,
  "devDependencies": {
    "mocha": "^8.0.1",
    "@rdfjs/data-model": "^1.1.2"
  },
  "scripts": {
    "test": "wasm-pack build --dev --target nodejs && mocha",
